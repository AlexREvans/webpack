{
  "name": "webpack",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "echo-loader": "^0.0.1",
    "html-webpack-plugin": "^2.30.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "scripts": {
    "zero": "cd app/0* && webpack --mode='development'",
    "example": "read -p 'Which example [1-4]? ' example && cd app/$example* && webpack",
    "start": "cd app/*dev* && webpack-dev-server --host 0.0.0.0",
    "build:dev": "cd app/*prod* && webpack --config webpack.dev.js && du -h dist/*",
    "build:prod": "cd app/*prod* && webpack --config webpack.prod.js && du -h dist/*",
    "deadcode": "grep 'increases bundle size' app/*prod*/dist/app.bundle.js"
  }
}
